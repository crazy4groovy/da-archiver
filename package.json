{
  "name": "da-archiver",
  "version": "0.9.1",
  "description": "Archive any DeviantArt user/group galleries and favourites locally.",
  "homepage": "https://github.com/crazy4groovy/da-archiver#readme",
  "main": "./src/index.js",
  "bin": {
    "da-archiver": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "tools:ubg": "node ./src/tools/update-best-galleries.js",
    "tools:dp": "node ./src/tools/discover-popular.js",
    "tools:sa": "node ./src/tools/sample-art.js",
    "test": "echo \"Error: no automated tests -- TISK TISK!\" && exit 1"
  },
  "engines": {
    "node": ">=14.x"
  },
  "keywords": [
    "deviantart"
  ],
